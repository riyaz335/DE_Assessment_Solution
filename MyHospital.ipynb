    }
   },
   "outputs": [],
   "source": [
    "df=spark.read.format(\"csv\").option(\"header\", \"true\").option(\"delimiter\", \"|\").schema(strct).load(\"dbfs:/mnt/raw/cdrdm/test_csv/Custumer.csv\").drop(\"_c0\",\"H\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "application/vnd.databricks.v1+cell": {
     "cellMetadata": {
